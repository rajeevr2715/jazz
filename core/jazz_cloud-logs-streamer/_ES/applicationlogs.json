{
  "template" : "applicationlogs",
  "settings" : {
    "index.refresh_interval" : "10s"
  },
  "mappings" : {
    "applicationlogs" : {
       "properties" : {
                        "timestamp" : { "type":"date","format":"dateOptionalTime" },
                        "request_id" : { "type": "keyword", "index": "false" },
                        "platform_log_group" : { "type":"keyword","index":"false" },
                        "platform_log_stream" : { "type":"keyword","index":"false" },
                        "servicename" : { "type": "keyword", "index": "false" },
                        "domain" : { "type": "keyword", "index": "false" },
                        "environment" : { "type": "keyword", "index": "false" },
                        "log_level" : { "type": "keyword", "index": "false" },
                        "message" : { "type": "keyword", "index": "false" }
       }
    }
  }
}
